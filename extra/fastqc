stringtie /media/sf_tutorial/PRJEB14936/ERR1549329.bam -p3 -G /media/sf_tutorial/references/ref.gtf -A /media/sf_tutorial/PRJEB14936/ERR1549329.tab -l -B -b /media/sf_tutorial/PRJEB14936/ballgown_in/ERR1549329 -e -o /media/sf_tutorial/PRJEB14936/ballgown/ERR1549329.gtf

