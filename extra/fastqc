wget https://www.bioinformatics.babraham.ac.uk/projects/fastqc/fastqc_v0.11.7.zip
mv /home/user/fastqc_v0.11.7.zip /home/user/AIDD/AIDD_tools/
unzip /home/user/AIDD/AIDD_tools/fastqc_v0.11.7.zip
rm /home/user/AIDD/AIDD_tools/fastqc_v0.11.7.zip
cp /home/user/AIDD/AIDD_tools/FastQC/factqc /home/user/AIDD/AIDD_tools/bin

cd "/media/sf_tutorial/PRJEB14936"
hisat2 -q -x /media/sf_tutorial/references/genome -p3 --dta-cufflinks -1 ERR1549325_trim_1.fastq -2 ERR1549325_trim_2.fastq -t --summary-file ERR1549325.txt -S ERR1549325.sam
fastqc ERR1549326_trim_1.fastq ERR1549326_trim_2.fastq
hisat2 -q -x /media/sf_tutorial/references/genome -p3 --dta-cufflinks -1 ERR1549326_trim_1.fastq -2 ERR1549326_trim_2.fastq -t --summary-file ERR1549326.txt -S ERR1549326.sam
fastq-dump ERR1549327.sra -I --split-files 
fastqc ERR1549327_1.fastq ERR1549327_2.fastq
fastx_trimmer -f 12 -l 72 -i ERR1549327_1.fastq -o ERR1549327_trim_1.fastq
fastx_trimmer -f 10 -l 72 -i ERR1549327_2.fastq -o ERR1549327_trim_2.fastq
fastqc ERR1549327_trim_1.fastq ERR1549327_trim_2.fastq
hisat2 -q -x /media/sf_tutorial/references/genome -p3 --dta-cufflinks -1 ERR1549327_trim_1.fastq -2 ERR1549327_trim_2.fastq -t --summary-file ERR1549327.txt -S ERR1549327.sam

fastq-dump ERR1549328.sra -I --split-files 
fastqc ERR1549328_1.fastq ERR1549328_2.fastq
fastx_trimmer -f 12 -l 72 -i ERR1549328_1.fastq -o ERR1549328_trim_1.fastq
fastx_trimmer -f 10 -l 72 -i ERR1549328_2.fastq -o ERR1549328_trim_2.fastq
fastqc ERR1549328_trim_1.fastq ERR1549328_trim_2.fastq
hisat2 -q -x /media/sf_tutorial/references/genome -p3 --dta-cufflinks -1 ERR1549328_trim_1.fastq -2 ERR1549328_trim_2.fastq -t --summary-file ERR1549328.txt -S ERR1549328.sam
fastq-dump ERR1549329.sra -I --split-files 
fastqc ERR1549329_1.fastq ERR1549329_2.fastq
fastx_trimmer -f 12 -l 72 -i ERR1549329_1.fastq -o ERR1549329_trim_1.fastq
fastx_trimmer -f 10 -l 72 -i ERR1549329_2.fastq -o ERR1549329_trim_2.fastq
fastqc ERR1549329_trim_1.fastq ERR1549329_trim_2.fastq
hisat2 -q -x /media/sf_tutorial/references/genome -p3 --dta-cufflinks -1 ERR1549329_trim_1.fastq -2 ERR1549329_trim_2.fastq -t --summary-file ERR1549329.txt -S ERR1549329.sam
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549324.sam OUTPUT=ERR1549324.bam SORT_ORDER=coordinate
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549325.sam OUTPUT=ERR1549325.bam SORT_ORDER=coordinate
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549326.sam OUTPUT=ERR1549326.bam SORT_ORDER=coordinate
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549327.sam OUTPUT=ERR1549327.bam SORT_ORDER=coordinate
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549328.sam OUTPUT=ERR1549328.bam SORT_ORDER=coordinate
java -jar /home/user/AIDD/AIDD_tools/picard.jar SortSam INPUT=ERR1549329.sam OUTPUT=ERR1549329.bam SORT_ORDER=coordinate

stringtie /media/sf_tutorial/PRJEB14936/ERR1549324.bam -p3 -G /media/sf_tutorial/references/ref.gtf -A /media/sf_tutorial/PRJEB14936/ERR1549324.tab -l -B -b /media/sf_tutorial/PRJEB14936/ballgown_in/ERR1549324 -e -o /media/sf_tutorial/PRJEB14936/ballgown/ERR1549324.gtf

stringtie /media/sf_tutorial/PRJEB14936/ERR1549325.bam -p3 -G /media/sf_tutorial/references/ref.gtf -A /media/sf_tutorial/PRJEB14936/ERR1549325.tab -l -B -b /media/sf_tutorial/PRJEB14936/ballgown_in/ERR1549325 -e -o /media/sf_tutorial/PRJEB14936/ballgown/ERR1549325.gtf
stringtie /media/sf_tutorial/PRJEB14936/ERR1549326.bam -p3 -G /media/sf_tutorial/references/ref.gtf -A /media/sf_tutorial/PRJEB14936/ERR1549326.tab -l -B -b /media/sf_tutorial/PRJEB14936/ballgown_in/ERR1549326 -e -o /media/sf_tutorial/PRJEB14936/ballgown/ERR1549326.gtf
stringtie /media/sf_tutorial/PRJEB14936/ERR1549327.bam -p3 -G /media/sf_tutorial/references/ref.gtf -A /media/sf_tutorial/PRJEB14936/ERR1549327.tab -l -B -b /media/sf_tutorial/PRJEB14936/ballgown_in/ERR1549327 -e -o /media/sf_tutorial/PRJEB14936/ballgown/ERR1549327.gtf
