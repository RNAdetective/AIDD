library("VennDiagram")
library("gdata")
library("gplots")
Lists <- read.csv("/media/sf_AIDD/Results/variant_calling/haplotype/level/protein_effect_impact/infection.csv")
head(Lists)
tail(Lists)
LS <- lapply(as.list(Lists), function(x) x[x != ""])
lapply(LS, tail)
names(LS) <- c("K048", "K054", "G010")
VENN.LIST <- LS
jpeg("/media/sf_AIDD/Results/variant_calling/haplotype/level/protein_effect_impact/infection.jpeg")
venn.plot <- venn.diagram(VENN.LIST, NULL, category.names=c("K048", "K054", "G010"), fill=c("yellow", "blue", "red"), alpha=c(0.5,0.5,0.5), cex = 2, cat.fontface=4)
grid.draw(venn.plot)
dev.off()
a <- venn(VENN.LIST, show.plot=TRUE)
str(a)
inters <- attr(a,"intersections")
lapply(inters, head)
sink("/media/sf_AIDD/Results/variant_calling/haplotype/level/protein_effect_impact/infectionVennDiagrams.txt")
inters
sink()
