library("gdata")
library("VennDiagram")
library("gplots")
gLists <- read.csv("/media/sf_AIDD/Results/DESeq2/level/differential_expression/venndiagrams/file_name.csv")
gLists$X <- NULL
head(gLists)
tail(gLists)
gLS <- lapply(as.list(gLists), function(x) x[x != ""])
lapply(gLS, tail)
names(gLS) <- c("regulationcell_lineclasslevel","regulationcell_lineclasstranscript")
VENN.LIST <- gLS
jpeg("/media/sf_AIDD/Results/DESeq2/level/differential_expression/venndiagrams/file_name.jpeg")
venn.plot <- venn.diagram(VENN.LIST, NULL, category.names=c("regulationcell_lineclasslevel","regulationcell_lineclasstranscript"), fill=c(set_colors), alpha=c(set_alpha), cex = 2, cat.fontface=4)
grid.draw(venn.plot)
dev.off()
a <- venn(VENN.LIST, show.plot=TRUE)
str(a)
inters <- attr(a,"intersections")
lapply(inters, head)
sink("/media/sf_AIDD/Results/DESeq2/level/differential_expression/venndiagrams/file_name.txt")
inters
sink()
