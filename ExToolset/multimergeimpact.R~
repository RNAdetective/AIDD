setwd("/media/sf_AIDD/Results/variant_calling/level/protein_effect_impact/condition/")
 
file_list <- list.files()
 
for (file in file_list){
       
  # if the merged dataset doesn't exist, create it
  if (!exists("dataset")){
    dataset <- read.csv(file)
  }
   
  # if the merged dataset does exist, append to it
  if (exists("dataset")){
    temp_dataset <-read.csv(file)
    dataset<-merge(dataset, temp_dataset, by="level_id")
    rm(temp_dataset)
  }
 dataset$A1BM_1.x <- NULL
 colnames(dataset)[2] <- "A1BM_1"
 write.csv(dataset, "/media/sf_AIDD/Results/variant_calling/level/protein_effect_impact/conditionlevelfinal.csv", row.names=FALSE)
