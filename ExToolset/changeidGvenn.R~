level_names <- read.csv("/media/sf_AIDD/index/level_names.csv")
data <- read.csv("/media/sf_AIDD/Results/DESeq2/level/differential_expression/regulationclass.csv")
colnames(data)[1] <- "level_name"
names <- merge(level_names, data, by="level_name")
data <- names[!duplicated(names$level_id), ]
write.csv(data, "/media/sf_AIDD/Results/DESeq2/level/differential_expression/regulationidclass.csv", row.names=FALSE)
data <- read.csv("/media/sf_AIDD/Results/DESeq2/level/differential_expression/regulationidclass.csv")
data$level_name <- NULL
colnames(data)[1] <- "regulationclasslevel"
data$level_id <- NULL
write.csv(data, "/media/sf_AIDD/Results/DESeq2/level/differential_expression/regulationidGlistclass.csv", row.names=FALSE)
